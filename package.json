{
  "name": "custom-ol",
  "version": "1.0.0",
  "description": "smaller custom openlayers build",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "ol-build": "node ./node_modules/openlayers/tasks/build.js ./config/ol-config.json ./lib/index.js",
    "ol-update-types": "C:\\Python34\\python.exe config\\modify_ts_d.py",
    "test-tsc": "cd test & tsc -w",
    "test-update": "cd test & npm update custom-ol & COPY /y node_modules\\custom-ol\\lib\\index.js js\\ol.js",
    "test-webpack": "cd test & webpack -d -w",
    "test-serve": "cd test & python -m http.server 5003"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glennvorhes/custom-ol.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/glennvorhes/custom-ol/issues"
  },
  "homepage": "https://github.com/glennvorhes/custom-ol#readme",
  "devDependencies": {
    "openlayers": "^3.18.2"
  }
}
