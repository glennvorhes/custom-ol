{
  "name": "custom-ol",
  "version": "1.0.0",
  "description": "smaller custom openlayers build",
  "main": "./lib/custom-ol-build.js",
  "typings": "./lib/custom-ol-build.d.ts",
  "files": [
    "lib",
    "ol",
    "src"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "ol-build": "node ./node_modules/openlayers/tasks/build.js ./src/ol-config.json ./ol/ol-build.js",
    "tsc": "tsc -w",
    "update-types": "C:\\Python33\\python.exe src\\modify_ts_d.py",
    "webpack-build": "webpack --config webpack.config_build.js -w",
    "serve-test": "python -m http.server 5003",
    "test-tsc": "cd test & tsc -w",
    "test-update": "cd test & npm update custom-ol & COPY /y node_modules\\custom-ol\\ol\\ol-build.js js\\"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glennvorhes/custom-ol.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/glennvorhes/custom-ol/issues"
  },
  "homepage": "https://github.com/glennvorhes/custom-ol#readme",
  "devDependencies": {
    "openlayers": "^3.18.2",
    "source-map-loader": "^0.1.6"
  }
}
